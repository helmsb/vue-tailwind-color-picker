{
  "name": "vue-tailwind-color-picker",
  "version": "1.0.6",
  "description": "Color picker for vue using tailwind2",
  "author": "advplyr",
  "main": "dist/vue-tailwind-color-picker.ssr.js",
  "browser": "dist/vue-tailwind-color-picker.esm.js",
  "module": "dist/vue-tailwind-color-picker.esm.js",
  "unpkg": "dist/vue-tailwind-color-picker.min.js",
  "files": [
    "dist/*",
    "src/**/*.vue"
  ],
  "sideEffects": false,
  "scripts": {
    "serve": "vue-cli-service serve dev/serve.js",
    "vue-build": "vue-cli-service build dev/serve.js",
    "start": "vue-cli-service start",
    "build": "cross-env NODE_ENV=production rollup --config build/rollup.config.js",
    "build:ssr": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format cjs",
    "build:es": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format es",
    "build:unpkg": "cross-env NODE_ENV=production rollup --config build/rollup.config.js --format iife"
  },
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@rollup/plugin-alias": "^3.1.1",
    "@rollup/plugin-babel": "^5.2.2",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.1",
    "@rollup/plugin-replace": "^2.3.4",
    "@tailwindcss/postcss7-compat": "^2.0.3",
    "@vue/cli-plugin-babel": "^4.5.10",
    "@vue/cli-service": "^4.5.10",
    "autoprefixer": "^9.8.6",
    "cross-env": "^7.0.3",
    "minimist": "^1.2.5",
    "postcss": "^7.0.35",
    "rollup": "^2.36.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-vue": "^5.1.9",
    "tailwindcss": "npm:@tailwindcss/postcss7-compat@^2.0.3",
    "vue": "^2.6.12",
    "vue-template-compiler": "^2.6.12"
  },
  "peerDependencies": {
    "vue": ">=2.6  <=3.2"
  },
  "engines": {
    "node": ">=12"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/advplyr/vue-tailwind-color-picker.git"
  },
  "homepage": "https://github.com/advplyr/vue-tailwind-color-picker",
  "keywords": [
    "color",
    "picker",
    "tailwind"
  ]
}